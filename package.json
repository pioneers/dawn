    "formik": "^2.2.6",
    "immutable": "3.8.2",
    "json-loader": "0.5.7",
    "keymirror": "0.1.1",
    "lodash": ">=4.17.21",
    "mobx": "^6.3.2",
    "mobx-react": "^7.2.0",
    "mousetrap": "1.6.1",
    "numeral": "2.0.6",
    "object-assign": "4.1.1",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "prop-types": "15.6.0",
    "protobufjs": "^6.10.2",
