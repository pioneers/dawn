{
  "name": "dawn",
  "version": "2.1.0",
  "description": "Frontend for PIE Robotics System",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/pioneers/dawn.git"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 140,
    "arrowParens": "always",
    "parser": "typescript",
    "bracketSpacing": true
  },
  "main": "build/main.js",
  "scripts": {
    "start": "electron build/main.js",
    "build": "tsc --build",
    "lint-actions": "eslint renderer/actions/**/*.ts",
    "lint-reducers": "eslint renderer/reducers/**/*.ts",
    "lint-types": "eslint renderer/types/**/*.ts",
    "lint-actions-fix": "eslint renderer/actions/**/*.ts --fix",
    "lint-reducers-fix": "eslint renderer/reducers/**/*.ts --fix",
    "lint-types-fix": "eslint renderer/types/**/*.ts --fix",
    "lint": "eslint '*/**/*.ts?(x)'",
    "lintfix": "eslint '*/**/*.ts?(x)' --fix",
    "build-protos": "pbjs -t static-module -w es6 -o protos/protos.js protos/*.proto && pbts -o protos/protos.d.ts protos/protos.js ",
    "watch": "webpack --mode development --watch --progress",
    "test": "mocha --require babel-core/register --recursive renderer/**/test/*.test.js",
    "release": "node release.ts",
    "version": "echo $npm_package_version"
  },
  "betterScripts": {
    "start": {
      "command": "cp ./protos/*.proto ./build && electron build/main.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "mkdir -p ./build && cp ./protos/*.proto ./build && webpack --mode production --progress",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "@types/minimist": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.3",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "better-npm-run": "0.1.0",
    "chai": "4.1.2",
    "electron-devtools-installer": "2.2.4",
    "electron-packager": "^15.0.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.5.1",
    "fs-extra": "5.0.0",
    "jszip": "3.1.5",
    "minimist": "1.2.3",
    "mocha": "5.0.0",
    "prettier": "^2.0.5",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "4.1.0",
    "webpack-cli": "2.0.10",
    "zip-webpack-plugin": "3.0.0"
  },
  "dependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/seedrandom": "^2.4.28",
    "@types/socket.io-client": "^1.4.33",
    "brace": "0.11.0",
    "bufferutil": "4.0.1",
    "create-react-class": "15.6.2",
    "electron": ">=1.7.16",
    "electron-json-storage": "4.0.2",
    "eslint-plugin-jsdoc": "^30.2.1",
    "immutable": "3.8.2",
    "json-loader": "0.5.7",
    "keymirror": "0.1.1",
    "lodash": ">=4.17.19",
    "mousetrap": "1.6.1",
    "numeral": "2.0.6",
    "object-assign": "4.1.1",
    "prop-types": "15.6.0",
    "protobufjs": ">=6.8.6",
    "react": "16.2.0",
    "react-ace": "5.9.0",
    "react-bootstrap": "0.32.0",
    "react-dom": ">=16.2.1",
    "react-immutable-proptypes": "2.1.0",
    "react-joyride": "1.11.1",
    "react-redux": "5.0.6",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "seedrandom": "2.4.3",
    "smalltalk": "2.5.1",
    "socket.io-client": "^2.3.0",
    "ssh2": "0.5.5",
    "superagent": "3.8.2",
    "utf-8-validate": "^5.0.2",
    "ws": "4.0.0"
  }
}
